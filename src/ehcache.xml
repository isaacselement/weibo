<?xml version="1.0" encoding="UTF-8"?>
<ehcache>
	<diskStore path="java.io.tmpdir" />
	<defaultCache 
		maxElementsInMemory="1000" 
		eternal="false"
		overflowToDisk="true" 
		timeToIdleSeconds="3600" 
		timeToLiveSeconds="7200"
		diskPersistent="false" />
	
	<cache name="org.hibernate.cache.UpdateTimestampsCache"   
       maxElementsInMemory="10000"   
       eternal="false"   
       timeToIdleSeconds="120"   
       timeToLiveSeconds="120"   
       overflowToDisk="true"/>   
   
   <cache name="org.hibernate.cache.StandardQueryCache"   
       maxElementsInMemory="10000"   
       eternal="false"   
       timeToIdleSeconds="120"   
       timeToLiveSeconds="120"   
       overflowToDisk="true"/>
	
	<cache name="message" 
		maxElementsInMemory="1000" 
		eternal="false"
		timeToIdleSeconds="3600" 
		timeToLiveSeconds="7200"
		overflowToDisk="true" />
	
	<cache name="member" 
		maxElementsInMemory="1000" 
		eternal="false"
		timeToIdleSeconds="3600" 
		timeToLiveSeconds="7200"
		overflowToDisk="true" />
		
		<cache name="photo" 
		maxElementsInMemory="1000" 
		eternal="false"
		timeToIdleSeconds="3600" 
		timeToLiveSeconds="7200"
		overflowToDisk="true" />
		
		<cache name="admin" 
		maxElementsInMemory="1000" 
		eternal="false"
		timeToIdleSeconds="3600" 
		timeToLiveSeconds="7200"
		overflowToDisk="true" />
		
		<cache name="posttopic" 
		maxElementsInMemory="1000" 
		eternal="false"
		timeToIdleSeconds="3600" 
		timeToLiveSeconds="7200"
		overflowToDisk="true" />
		
		<cache name="reply" 
		maxElementsInMemory="1000" 
		eternal="false"
		timeToIdleSeconds="3600" 
		timeToLiveSeconds="7200"
		overflowToDisk="true" />
</ehcache>
